{% extends 'base-front.html.twig' %}



{% block hero%} {% endblock %}

{% block about %}{% endblock %}

{% block prog %}{% endblock %}

{% block coach %}{% endblock %}

{% block contact %}{% endblock %}

{% block title %}Event{% endblock %}

{% block body %}
    
    <section class="class section" id="class">

				<div class="container">
					<div class="row">

						<div class="col-lg-12 col-12 text-center">
<h1>évenement</h1>
   <table class="table table-bordered table-responsive schedule-table" data-aos="fade-up" data-aos-delay="300">
    <thead class="thead-light">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ event.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ event.nom }}</td>
            </tr>
    
            <tr>
                <th>Date</th>
                <td>{{ event.date ? event.date|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>ticket</th>
                <td>{{ event.ticket }}</td>
            </tr>
            <tr>
                <th>image</th>
                <td><img src="{{ asset('picture/' ~ event.image) }}" alt="" width="90" height="90"></td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('evt_index') }}">Retour à la liste</a>
    

    <a href="{{ path('evt_edit', {'id': event.id}) }}">Modifier</a>
{{ include('evt/_delete_form.html.twig') }}  
               </div>
					</div>
				</div>
			</section>
{% endblock %}
