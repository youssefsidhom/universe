{% extends 'base-front.html.twig' %}
{% block title %}MyCoach
{% endblock %}


{% block hero%} {% endblock %}

{% block about %}{% endblock %}

{% block prog %}{% endblock %}

{% block coach %}{% endblock %}

{% block contact %}{% endblock %}

{% block body %}
  <section class="class section" id="class">

				<div class="container">
					<div class="row">
   <div class="col-lg-12 col-12 text-center">

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Prix</th>
                <th>nom</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for ticket in tickets %}
            <tr>
                <td>{{ ticket.id }}</td>
                <td>{{ ticket.prix }}</td>
                <td>{{ ticket.nom }}</td>
                <td>
                    <a href="{{ path('ticket_show', {'id': ticket.id}) }}">afficher</a>
                    <a href="{{ path('ticket_edit', {'id': ticket.id}) }}">modifier</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('ticket_new') }}">Cr√©er un nouveau </a>
{% endblock %}
